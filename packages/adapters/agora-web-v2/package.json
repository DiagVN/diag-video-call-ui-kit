{
  "name": "@diagvn/agora-web-adapter-v2",
  "version": "2.0.11",
  "description": "Agora Web SDK adapter for DiagVN Video Call UI Kit V2 - Full feature support",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "agora-rtc-sdk-ng": "^4.20.0",
    "agora-rtm-sdk": "^2.1.0",
    "agora-extension-virtual-background": "^1.1.3",
    "agora-extension-beauty-effect": "^1.0.2-beta",
    "agora-extension-ai-denoiser": "^1.1.0"
  },
  "peerDependenciesMeta": {
    "agora-extension-virtual-background": {
      "optional": true
    },
    "agora-extension-beauty-effect": {
      "optional": true
    },
    "agora-extension-ai-denoiser": {
      "optional": true
    }
  },
  "dependencies": {
    "@diagvn/video-call-core-v2": "^2.0.0"
  },
  "devDependencies": {
    "agora-rtc-sdk-ng": "^4.20.0",
    "agora-rtm-sdk": "^2.1.0",
    "agora-extension-virtual-background": "^1.1.3",
    "agora-extension-beauty-effect": "^1.0.2-beta",
    "agora-extension-ai-denoiser": "^1.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "keywords": [
    "agora",
    "webrtc",
    "video-call",
    "vue",
    "adapter"
  ],
  "sideEffects": false,
  "author": "DiagVN",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/diagvn/video-call-ui-kit"
  }
}
