# Playground Environment Variables
# Copy this file to .env.local and fill in your values

# Agora App ID (required for Agora mode)
VITE_AGORA_APP_ID=your_agora_app_id_here

# Token server URL (optional, for production use)
VITE_TOKEN_SERVER_URL=https://your-token-server.com/rtc-token

# Default mode: 'mock' or 'agora'
VITE_DEFAULT_MODE=mock

# Default language: 'vi' or 'en'
VITE_DEFAULT_LANGUAGE=vi
